{
  "name": "heimrichhannot/contao-location-bundle",
  "type": "contao-bundle",
  "description": "This bundle offers functionality concerning location handling for the Contao CMS.",
  "license": "LGPL-3.0-or-later",
  "require": {
    "php": "^7.4",
    "contao/core-bundle": "^4.9",
    "heimrichhannot/contao-utils-bundle": "^2.196"
  },
  "autoload": {
    "psr-4": {
      "HeimrichHannot\\LocationBundle\\": "src/"
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "extra": {
    "contao-manager-plugin": "HeimrichHannot\\LocationBundle\\ContaoManager\\Plugin"
  }
}
